<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- This is image textview -->

    <!-- This is welcome textview -->

    <ImageView
        android:id="@+id/image"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#090030"
        android:scaleType="centerCrop"
        android:src="@drawable/background_image"
        app:layout_constraintBottom_toTopOf="@id/gl_34pc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/community"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="@string/community_ek3_string"
        android:textColor="@color/login_ek3_color"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image" />

    <!-- Start white rounded tiles -->

    <ImageView
        android:id="@+id/rectangle_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:contentDescription="rectangle_1"
        android:src="@drawable/rectangle_2_ek2"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_34pc" />

    <ImageView
        android:id="@+id/rectangle_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:contentDescription="rectangle_2"
        android:src="@drawable/rectangle_2_ek2"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_34pc" />

    <ImageView
        android:id="@+id/rectangle_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:contentDescription="rectangle_3"
        android:src="@drawable/rectangle_2_ek2"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc" />

    <ImageView
        android:id="@+id/rectangle_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:contentDescription="rectangle_4"
        android:src="@drawable/rectangle_2_ek2"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc" />

    <!-- End white rounded tiles -->

    <!-- Start text on white rounded tiles  -->
    <TextView
        android:id="@+id/compete_with_friends"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/compete_with_friends_string"
        android:textColor="@color/compete_with_friends_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toTopOf="@+id/gl_34pc"
        app:layout_constraintVertical_bias="0.30" />

    <TextView
        android:id="@+id/month"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/leaderboard_string"
        android:textColor="@color/leaderboard_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_34pc"
        app:layout_constraintVertical_bias="0.40" />

    <TextView
        android:id="@+id/workout_with_friends"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/workout_with_friends_string"
        android:textColor="@color/workout_with_friends_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc"
        app:layout_constraintVertical_bias="0.30" />

    <TextView
        android:id="@+id/share_your_progress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/share_your_progress_string"
        android:textColor="@color/share_your_progress_color"
        android:textSize="17sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc"
        app:layout_constraintVertical_bias="0.30" />

    <!-- End text on white rounded tiles  -->

    <!-- Start line vector on white rounded tiles -->

    <ImageView
        android:id="@+id/vector_red"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="70dp"
        android:contentDescription="rectangle_1"
        android:src="@drawable/vector_red"
        android:padding="30dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_34pc" />

    <ImageView
        android:id="@+id/vector_orange"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="70dp"
        android:contentDescription="rectangle_1"
        android:src="@drawable/vector_orange"
        android:padding="30dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_60pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_34pc" />

    <ImageView
        android:id="@+id/vector_green"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="70dp"
        android:contentDescription="rectangle_1"
        android:src="@drawable/vector_green"
        android:padding="30dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_44pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc" />

    <ImageView
        android:id="@+id/vector_blue"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="70dp"
        android:contentDescription="rectangle_1"
        android:src="@drawable/vector_blue"
        android:padding="30dp"
        app:layout_constraintBottom_toTopOf="@+id/gl_86pc"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gl_56pc"
        app:layout_constraintTop_toBottomOf="@+id/gl_60pc"/>

    <!-- End line vector on white rounded tiles -->

    <!-- Start of navigation-->
    <View
        android:id="@+id/ellipse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/ellipse_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/gl_56pc"
        app:layout_constraintStart_toStartOf="@+id/gl_44pc"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc" />

    <ImageButton
        android:id="@+id/vector"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:contentDescription="vector"
        android:src="@drawable/vector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc"
        app:layout_constraintVertical_bias="0.422"
        android:background="@android:color/transparent"
        android:padding="15dp"/>


    <ImageButton
        android:id="@+id/xmlid_837_ek1"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:contentDescription="xmlid_837_ek1"
        android:src="@drawable/xmlid_837_ek1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/gl_10pc"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc"
        app:layout_constraintVertical_bias="0.0"
        android:background="@android:color/transparent"
        android:padding="15dp"/>

    <ImageButton
        android:id="@+id/xmlid_1093"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:src="@drawable/xmlid_1093"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/gl_90pc"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc"
        android:background="@android:color/transparent"
        android:padding="15dp"/>

    <ImageButton
        android:id="@+id/xmlid_1161_ek1"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:contentDescription="xmlid_1161_ek1"
        android:src="@drawable/xmlid_1161_ek1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/xmlid_1093"
        app:layout_constraintStart_toEndOf="@+id/ellipse"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc"
        android:background="@android:color/transparent"
        android:padding="15dp"/>


    <ImageButton
        android:id="@+id/xmlid_1161_ek3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        android:padding="15dp"
        android:src="@drawable/xmlid_852"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ellipse"
        app:layout_constraintStart_toEndOf="@+id/xmlid_837_ek1"
        app:layout_constraintTop_toTopOf="@+id/gl_91pc" />

    <!-- End of navigation-->

    <!-- Start of guidelines  -->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_34pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.34" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_10pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.10" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_90pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.90" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_91pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.91" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_56pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.56" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_44pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.44" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_60pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_86pc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.86" />

    <!-- End of guidelines  -->

</androidx.constraintlayout.widget.ConstraintLayout>